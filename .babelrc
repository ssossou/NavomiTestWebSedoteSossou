{
  "presets": ["stage-2", "env", "react"],
  "plugins": ["transform-object-rest-spread", "transform-class-properties"],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        "add-module-exports"
      ]
    }
  }
}
